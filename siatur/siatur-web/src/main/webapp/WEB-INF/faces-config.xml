<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">

	<application>
		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>


		<navigation-rule>
			<from-view-id>*</from-view-id>
			<navigation-case>
				<from-action>home</from-action>
				<to-view-id>/faces/home.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_usuarios</from-action>
				<to-view-id>/faces/pages/gestionar_usuarios.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_roles</from-action>
				<to-view-id>/faces/pages/gestionar_roles.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_clientes</from-action>
<!-- 				<from-outcome>success</from-outcome> -->
				<to-view-id>/faces/pages/gestionar_clientes.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_requisitos</from-action>
				<to-view-id>/faces/pages/gestionar_requisitos.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_temporadas</from-action>
				<to-view-id>/faces/pages/gestionar_temporadas.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>



	</application>

</faces-config>
